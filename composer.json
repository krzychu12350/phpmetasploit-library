{
    "name": "krzychu12350/phpmetasploit",
    "description": "This composer plugin allow to interact with Metasploit either through msfrpcd or the msgrpc plugin in msfconsole.",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzychu12350",
            "email": "krzysztofkaras98@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Krzychu12350\\Phpmetasploit\\": "src",
            "Krzychu12350\\Phpmetasploit\\MsfRpcClient\\": "",
            "Krzychu12350\\Phpmetasploit\\PluginInstaller\\": ""
        }
    },
    "extra": {
        "class": "Krzychu12350\\Phpmetasploit\\PluginInstaller"
    },
    "scripts": {
        "test": "@php --version",
        "generateOld": "Krzychu12350\\Phpmetasploit\\PluginInstaller::createApiMethods",
        "generate": "Krzychu12350\\Phpmetasploit\\MsfLibraryGenerator::generateLibrary"
    },
    "require": {
        "php": ">=8.1.2",
        "ext-curl": "*",
        "lib-curl-zlib": "^1.2",
        "curl/curl": "^2.3",
        "rybakit/msgpack": "^0.9.1",
        "nette/php-generator": "^4.0",
        "fabpot/goutte": "v4.0.2",
        "composer-plugin-api": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.21",
        "composer/composer": "^2.0"
    }
}
