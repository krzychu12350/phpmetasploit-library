{
    "name": "krzychu12350/phpmetasploit",
    "description": "This library allow to interact with Metasploit either through msfrpcd or the msgrpc plugin in msfconsole.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzychu12350",
            "email": "krzysztofkaras98@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1.2",
        "ext-curl": "*",
        "lib-curl-zlib": "^1.2",
        "curl/curl": "^2.3",
        "rybakit/msgpack": "^0.9.1",
        "nette/php-generator": "^4.0",
        "fabpot/goutte": "v4.0.2",
        "werbfred/composer-package-updater": "v1.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.21",
        "neronmoon/scriptsdev": "v0.1.9"
    },
    "autoload": {
        "psr-4": {
            "Krzychu12350\\Phpmetasploit\\": "src",
            "Krzychu12350\\Phpmetasploit\\MsfRpcClient\\": "",
            "ComposerPackageUpdater\\composer\\": "./vendor/werbfred/composer-package-updater/src/composer"
        },
        "psr-0": {
            "ComposerScript\\Installer" : "",
            "ComposerScript\\FileGenerator" : ""
        }
    },
    "scripts": {
        "test": [
            "@php --version"
        ],
        "generate-files": "Krzychu12350\\Phpmetasploit\\MsfRpcClient::createApiMethods",
        "post-cmd-install": "ComposerScript\\Installer::postPackageUpdate",
        "post-cmd-update": "ComposerScript\\Installer::postPackageUpdate"
    },
    "extra": {
        "scripts-dev": {
            "post-install-cmd": [
                "Krzychu12350\\Phpmetasploit\\MsfRpcClient::createApiMethods"
            ],
            "post-update-cmd": "Krzychu12350\\Phpmetasploit\\MsfRpcClient::createApiMethods",
            "test": "phpunit"
        }
    },
    "config": {
        "allow-plugins": {
            "neronmoon/scriptsdev": true
        }
    }
}
