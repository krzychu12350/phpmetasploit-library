{
    "name": "krzychu12350/phpmetasploit",
    "description": "This library allow to interact with Metasploit either through msfrpcd or the msgrpc plugin in msfconsole.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzychu12350",
            "email": "krzysztofkaras98@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1.2",
        "ext-curl": "*",
        "lib-curl-zlib": "^1.2",
        "curl/curl": "^2.3",
        "rybakit/msgpack": "^0.9.1",
        "nette/php-generator": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Krzychu12350\\Phpmetasploit\\": "src"
        }
    },
    "scripts": {
            "test": [
                "@php --version"
            ],
        "generate-files": "src/MsfRpcClient.php::createApiMethods",
        "post-package-install": [
            "@composer dump-autoload",
            "@php src\\script.php"
        ]
    }
}
